{
    "extends": "./eslint/config",
    "env": {
      "node": true,
      "mocha": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "no-underscore-dangle": 0,
      "max-classes-per-file": 0,
      "class-methods-use-this": 0,
      "no-restricted-syntax": [
        0,
        "ForOfStatement"
      ],
      "comma-dangle": [
        2,
        {
          "objects": "never",
          "arrays": "never"
        }
      ]
    }
  }
  